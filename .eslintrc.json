{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "array-callback-return": "off",
        "no-else-return":"off",
        "no-console":"off"
       
        
    }
}